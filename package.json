{
  "name": "stelllar",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-config-custom": "*",
    "eslint-config-nestjs": "*",
    "prettier": "^2.7.1",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
